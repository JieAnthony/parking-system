{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-swoole": "*",
        "bensampo/laravel-enum": "^3.2",
        "dcat/laravel-admin": "2.*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "godruoyi/php-snowflake": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "mews/purifier": "^3.3",
        "mymediamagnet/uniquewith-validator": "^3.4",
        "orangehill/iseed": "^3.0",
        "overtrue/laravel-lang": "^4.2",
        "overtrue/laravel-options": "^2.0",
        "overtrue/laravel-wechat": "^6.0",
        "simps/mqtt": "^1.1",
        "tucker-eric/eloquentfilter": "^2.4",
        "tymon/jwt-auth": "^1.0",
        "yansongda/pay": "^2.10"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "beyondcode/laravel-dump-server": "^1.7",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "matt-allan/laravel-code-style": "^0.6.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "overtrue/laravel-query-logger": "^2.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-fix": "php-cs-fixer fix",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
